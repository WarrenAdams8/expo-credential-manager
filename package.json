{
  "name": "expo-credential-manager",
  "version": "0.1.1",
  "description": "Expo module exposing Android Credential Manager (passkeys and passwords).",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WarrenAdams8/expo-credential-manager.git"
  },
  "bugs": {
    "url": "https://github.com/WarrenAdams8/expo-credential-manager/issues"
  },
  "homepage": "https://github.com/WarrenAdams8/expo-credential-manager#readme",
  "main": "build/index.js",
  "module": "build/index.js",
  "types": "build/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "jest",
    "prepack": "npm run build"
  },
  "files": [
    "android",
    "ios",
    "build",
    "src",
    "app.plugin.js",
    "expo-module.config.json"
  ],
  "keywords": [
    "expo",
    "android",
    "credential-manager",
    "passkey",
    "webauthn"
  ],
  "peerDependencies": {
    "expo": ">=50.0.0",
    "expo-modules-core": ">=1.11.0",
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.5.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "clearMocks": true
  }
}
